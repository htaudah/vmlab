<?xml version="1.0" encoding="utf-8"?>
<properties>
  <property name="ADDLOCAL" value="AirWatch_API_Website,AirWatch_Cloud_Messaging,Device_Services,Device_Services_Windows_Services,AirWatch_Messaging_Service_DS,App_Catalog_DS,Common,Default_Website_DS,Device_Management_DS,Device_Services_Website,Event_Change_Queue_Monitor_DS,Interrogator_Queue_Monitor,Interrogator_Server,Log_Manager_Queue_Monitor,Master_Queue_Service,Self_Service_Portal,MEG_Queue_Service_DS,Cisco_ISE_Integration_DS,Provisioning_Package_Service" />
  <!-- Installation Directory -->
  <property name="INSTALLDIR" value="C:\AirWatch\" />
  <!-- SQL Server Host Name -->
  <property name="IS_SQLSERVER_SERVER" value="MainAG" />
  <!-- SQL Server Authentication Type; Windows Auth = 0, SQL Server Auth = 1 -->
  <property name="IS_SQLSERVER_AUTHENTICATION" value="1" />
  <!-- SQL Server Username -->
  <property name="IS_SQLSERVER_USERNAME" value="uem_db_acct" />
  <!-- SQL Server Database Name -->
  <property name="IS_SQLSERVER_DATABASE" value="WorkspaceONEUEM" />
  <!-- SQL Server Password -->
  <property name="IS_SQLSERVER_PASSWORD" value="{{ database_password }}" />
  <!-- If 0 is selected for Windows Auth, this is the Windows Auth Username -->
  <property name="IS_NET_API_LOGON_USERNAME" value="" />
  <!-- If 0 is selected for Windows Auth, this is the Windows Auth Password -->
  <property name="IS_NET_API_LOGON_PASSWORD" value="" />
  <!-- AirWatch Console DNS Address -->
  <property name="AWSERVER" value="uemc.{{ domain_name }}" />
  <!-- AirWatch Console Scheme; HTTPS = 1, HTTP = empty string -->
  <property name="AWSERVERSCHEME" value="1" />
  <!-- AirWatch DeviceServices DNS Address -->
  <property name="AWSERVERDS" value="uds.{{ domain_name }}" />
  <!-- AirWatch DeviceServices Scheme; HTTPS = 1, HTTP = empty string -->
  <property name="AWSERVERDSSCHEME" value="1" />
  <!-- API server SSL offloading status; SSL offloaded = 1, Not SSL offloaded = empty string -->
  <property name="AWAPISSLOFFLOADED" value="" />
  <!-- Target IIS Website for publishing AirWatch -->
  <property name="AWWEBSITE" value="Default Web Site" />
  <!-- AWCM Server Listening Interface -->
  <property name="ACMSERVERIP" value="0.0.0.0" />
  <!-- AWCM Server Listening Port -->
  <property name="ACMSERVERPORT" value="2001" />
  <!-- AWCM Clustering Model; Implicit = 1, Explicit = 2 -->
  <property name="AWCMCLUSTER" value="1" />
  <!-- Comma separated list of nodes for Explicit Clustering -->
  <property name="AWCMNODES" value="" />
  <!-- AWCM server SSL offloading status; SSL offloaded = 1, Not SSL offloaded = empty string -->
  <property name="AWCMSSLOFFLOADED" value="1" />
  <!-- Use your own publicly trusted SSL cert for AWCM; If providing cert = empty string; Use a self signed cert = 1 -->
  <property name="AWCMUSEOWNCERT" value="1" />
  <!-- Absolute path to cert for AWCM, if providing own -->
  <property name="AWCMCERT" value="" />
  <!-- Password for AWCM certificate, if providing own -->
  <property name="AwcmCertPass" value="" />
  <!-- SAAS ONLY; Absolute path of smb share; EXAMPLE: smb://mysmbshare.com/share -->
  <property name="AWCMSMBSHARE" value="" />
  <!-- SAAS ONLY; The doman of the smb user -->
  <property name="AWCMSMBDOMAIN" value="" />
  <!-- SAAS ONLY; The smb username -->
  <property name="AWCMSMBUSERNAME" value="" />
  <!-- SAAS ONLY; The smb username password -->
  <property name="AWCMSMBPASSWORD" value="" />
  <!-- SAAS ONLY; Absolute path to the JWT truststore -->
  <property name="AWCMJWTPATH" value="" />
  <!-- SAAS ONLY; Password for the JWT truststore -->
  <property name="AWCMJWTPASSWORD" value="" />
  <!-- Company Name for GEM -->
  <property name="AWGEMCOMPANY" value="{{ orgname }}" />
  <!-- GEM Environment Type; Options: Production, POC, UAT -->
  <property name="AWGEMENVTYPE" value="Production" />
  <!-- Leave empty unless this is a Partner install -->
  <property name="AWPARTNERID" value="" />
  <!-- Installer will not make a backup copy of the previous AirWatch version folder = true, Backup will occur = false-->
  <property name="AWIGNOREBACKUP" value="true" />
</properties>
